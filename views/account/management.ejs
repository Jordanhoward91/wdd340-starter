<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Account Management</title>
</head>
<body>
  <header>
    <%= nav %>
  </header>
  <main>
    <% if (locals.flash) { %>
      <p><%= flash.notice %></p>
    <% } %>
    <% if (errors) { %>
      <ul>
        <% errors.forEach(error => { %>
          <li><%= error %></li>
        <% }) %>
      </ul>
    <% } %>
    <h1>You're logged in</h1>
  </main>
</body>
</html>
